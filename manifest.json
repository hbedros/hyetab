{
  "manifest_version": 2,
  "name": "HyeTab",
  "description": "Tab to donate to armenia",
  "version": "1.0.0",
  "author": "hbe",
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": [
    "bookmarks",
    "https://source.unsplash.com/",
    "management",
    "history",
    "storage",
    "tabs",
    "storage",
    "http://*/*",
    "https://*/*",
    "<all_urls>",
    "https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"
  ],
  "content_security_policy": "script-src 'self' 'sha256-WOdSzz11/3cpqOdrm89LBL2UPwEU9EhbDtMy2OciEhs='; script-src https://pagead2.googlesyndication.com/pagead/js/adsbygoogle.js 'self' 'unsafe-eval'; object-src 'self'"

}
