{
    "manifest_version": 2,
    "name": "Hye Tab",
    "description": "Tab to donate to armenia",
    "version": "1.0.0",
    "author": "hbe",
    "permissions": [
      "https://cdn.jsdeliver.net/npm/vue/dist/vue.js"
  ],
  "background": {
      "scripts": ["popup.js"],
      "persistent": false,

    "chrome_url_overrides": {
      "newtab": "new_tab.html"
    },
    "icons": {
      "16": "icons/32.png",
      "32": "icons/32.png",
      "128": "icons/32.png"
    },

  "permissions": ["https://source.unsplash.com/"], 
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://cdn.jsdelivr.net; object-src 'self'"
  }
}